{
  "name": "react-boilerplate-v1",
  "version": "1.0.0",
  "description": "a basic react boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "yarn build-server && yarn build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js",
    "test": "jest ./src"
  },
  "repository": "https://github.com/GBarrBrown/react-boilerplate-v1.git",
  "author": "gbarrbrown <gbb7899@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^24.3.1"
  }
}
